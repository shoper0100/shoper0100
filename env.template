# Private key for deployer wallet (REQUIRED)
PRIVATE_KEY=your_private_key_here

# Contract deployment parameters (REQUIRED)
FEE_RECEIVER_ADDRESS=0x0000000000000000000000000000000000000000
ROYALTY_ADDRESS=0x0000000000000000000000000000000000000000
OWNER_ADDRESS=0x0000000000000000000000000000000000000000

# Starting user ID for the contract (OPTIONAL - defaults to 36999)
DEFAULT_REFER_ID=36999

# Network RPC URLs
OPBNB_RPC_URL=https://opbnb-mainnet-rpc.bnbchain.org
OPBNB_TESTNET_RPC_URL=https://opbnb-testnet-rpc.bnbchain.org

# BSCScan API key for contract verification (OPTIONAL)
BSCSCAN_API_KEY=your_bscscan_api_key_here

# Frontend environment variables (update after deployment)
NEXT_PUBLIC_CONTRACT_ADDRESS=0xae9a7740352Bd4E3f23D7725AcF7D91b8091059D
NEXT_PUBLIC_DEFAULT_REFER=36999
NEXT_PUBLIC_CHAIN_ID=204
NEXT_PUBLIC_RPC_URL=https://opbnb-mainnet-rpc.bnbchain.org
NEXT_PUBLIC_EXPLORER_URL=https://opbnb.bscscan.com
